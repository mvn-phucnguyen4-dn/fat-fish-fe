{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:react/recommended", "plugin:prettier/recommended"],
  "plugins": ["eslint-plugin-prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "react/prop-types": "off",
    "react/no-unescaped-entities": 0
  }
}
